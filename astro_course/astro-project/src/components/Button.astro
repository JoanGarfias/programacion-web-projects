---

interface Props {
  texto: string;
}

const { texto } = Astro.props;

---
<button>
{texto}
</button>
<slot name="icono"/>
<slot name="precio"/>

<style>
button{
    all: unset;
    background-color: #4a7aba;
    color: #fff;
    width: 100px;
    height: 50px;
    text-align: center;
}
</style>